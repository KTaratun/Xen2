#: import FadeTransition kivy.uix.screenmanager.FadeTransition
#: import isfile os.path.isfile

ScreenManagement:
	id: sManager
	transition: FadeTransition()
	TitleScreen:
		id: title
		name: "title"
		manager: sManager
	CharacterSelectScreen:
		id: charSelect
		name: "charSelect"
		manager: sManager
	ClassSelectScreen:
		id: class
		name: "class"
		manager: sManager
	CardInfoScreen:
		id: cards
		name: "cards"
		manager: sManager
	CharacterStatsScreen:
		id: stats
		name: "stats"
		manager: sManager
	ElementSelectScreen:
		id: elements
		name: "elements"
		manager: sManager
	LoadoutScreen:
		id: loadout
		name: "loadout"
		manager: sManager
	CharacterScreen:
		id: character
		name: "character"
		manager: sManager
	
<Label>:
	pos_hint: {"center_x": .5, "center_y": .85}
	font_size: 40
	
<Button>:
	font_size: 20
	size_hint: 0.35, 0.15

<Screen>:
	canvas:
		Color:
			rgba: .8, .8, .8, 1
		Rectangle:
			pos: self.pos
			size: self.size
	Button:
		id: back
		text: "Back"
		size_hint: .15, .1
		pos_hint: {"center_x": .1, "center_y": .08}
		on_release: app.root.current = root.parent.backStack.pop()

<TitleScreen>:
	Label:
		text: "Xen2 Alpha"
	Button:
		on_release: 
			app.root.path = open("C:/Users/Kurt/IdeaProjects/Xen2CharacterSheet2/char1.txt", "r")
			app.root.LoadData()
			app.root.CharScreenUpdate(app.root.ids.character)
			app.root.DeckAndGearInit()
			root.parent.nextscreen(last="title", next="character")
		text: "Player"
		pos_hint: {"center_x": .5, "center_y": .55}
		size_hint: 0.5, 0.2
	Button:
		on_release: print(self.parent.children[3].text)
		text: "Game Master"
		pos_hint: {"center_x": .5, "center_y": .25}
		size_hint: 0.5, 0.2
		
